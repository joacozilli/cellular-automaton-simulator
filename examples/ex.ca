Automaton myCA {
    States := one : white | two : white

    Neighborhood := (1,0) | (0,2) | (0,1)

    Transition {
        let one = 12
        in
            case {
                cell == nei(2) and #one in [#one] and #one == #one : { #one }
                otherwise : { #one }
                }

    }

    Default := one
}
